<!DOCTYPE html>
<html>
<head>
	<title>Test Template</title>
</head>
<body>
	<h1>@Model.Name</h1>

	<nav>
        @if (Model.IsLoggedIn) {
			<a>Sign Out</a>
        } else { 
			<a>Sign In</a>
		}
	</nav>
    <ul>
        @foreach (var role in Model.Roles)
        {
            <li>@role</li>
        }
    </ul>

</body>
</html>